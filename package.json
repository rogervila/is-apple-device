{
  "name": "is-apple-device",
  "author": "Roger Vil√† <rogervila@me.com> (https://rogervila.es)",
  "version": "1.2.0",
  "bugs": {
    "url": "https://github.com/rogervila/is-apple-device/issues"
  },
  "homepage": "https://github.com/rogervila/is-apple-device#readme",
  "keywords": [
    "apple",
    "ipad",
    "iphone",
    "ipod",
    "mac",
    "osx",
    "ios"
  ],
  "scripts": {
    "build": "vite build",
    "lint:eslint": "eslint --cache . --ext .js,.cjs,.ts",
    "lint:ts": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:ts",
    "lint:fix": "npm run lint:eslint -- --fix",
    "test": "vitest run",
    "prepare": "husky install",
    "commit": "commit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/is-apple-device.umd.cjs",
  "module": "./dist/is-apple-device.js",
  "exports": {
    ".": {
      "import": "./dist/is-apple-device.js",
      "require": "./dist/is-apple-device.umd.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/prompt-cli": "19.3.1",
    "@lokalise/eslint-config-frontend": "4.6.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.0.6",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "9.0.11",
    "prettier": "2.8.8",
    "semantic-release": "24.0.0",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vite-plugin-dts": "3.9.1",
    "vitest": "1.6.0"
  }
}
